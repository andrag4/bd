<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="521" Width="1635" >
    <DockPanel>
        <Button x:Name="button1" Content="Button" DockPanel.Dock="Right" Click="button1_Click"/>
        <DataGrid x:Name="dataGrid1" Width="1368" d:ItemsSource="{d:SampleData ItemCount=5}" Height="237"/>
        <TextBox x:Name="textBox1" TextWrapping="Wrap" Text="TextBox" DockPanel.Dock="Bottom" VerticalContentAlignment="Bottom"/>
        <TextBox x:Name="textBox2" TextWrapping="Wrap" Text="TextBox" Width="100"/>
    </DockPanel>
</Window>
